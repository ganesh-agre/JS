let words = [
  "sai",
  "ganesh",
  "sharad",
  "rashmi",
  "lex",
  "mac",
  "ritu",
  "pi",
  "rameshwar",
];

//ascending orders
// let sortedWords = words.sort((a, b) => {
//   return a.length - b.length
// });

//descending orders
let sortedWords = words.sort((a, b) => {
  return b.length - a.length;
});

console.log(sortedWords);
